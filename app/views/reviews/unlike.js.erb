$('.unlike_review').bind('ajax:success', function(){
   $(this).parent().parent().find('.vote_count').html('<%= escape_javascript @review.votes_for.size.to_s %>');
   $(this).closest('.unlike_review').hide();
   $(this).closest('.votes').html(' <%= link_to "Like", like_review_path(@review), remote: true, method: :get, class: 'like_review btn btn-primary' %>');
});